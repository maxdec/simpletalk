{
  "name": "node-react-starter",
  "version": "0.0.0",
  "description": "Node-React Starter project. It will help you start a new project quickly: ExpressJS, Socket.io, Redis connection and React + Browserify",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "test": "mocha --recursive",
    "build": "browserify -t reactify public/js/main.jsx -o public/bundle.js",
    "watch": "watchify -t reactify public/js/main.jsx -o public/bundle.js --debug -v"
  },
  "author": "Maxence Decrosse",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.3.0",
    "easyrtc": "^1.0.13",
    "ejs": "^1.0.0",
    "express": "^4.12.0",
    "flux": "~2.0.1",
    "hiredis": "^0.1.17",
    "keymirror": "~0.1.1",
    "morgan": "^1.1.1",
    "object-assign": "~2.0.0",
    "peer": "^0.2.8",
    "peerjs": "^0.3.14",
    "q": "^1.0.1",
    "react": "~0.12.2",
    "reactify": "^1.0.0",
    "redis": "^0.10.3",
    "serve-favicon": "^2.0.0",
    "socket.io": "^1.0.6",
    "socket.io-client": "^1.3.4"
  },
  "devDependencies": {
    "chai": "^1.9.1",
    "chai-as-promised": "^4.1.1",
    "heapdump": "^0.2.9",
    "sinon": "^1.10.3"
  },
  "browserify": {
    "transform": [
      [
        "reactify"
      ]
    ]
  }
}
